{	
	"express":{
		"db": {
			"def": "prot",
			"list": {
				"prot": {
					"drosoph_aa": {
						"path": "/opt/db/drosoph.aa",
						"origin": "ftp://ftp.ncbi.nlm.nih.gov/blast/db/FASTA/drosoph.aa.gz"
					}
				},
				"nucl": {
					"drosoph_nt": {
						"path": "/opt/db/drosoph.nt",
						"origin": "ftp://ftp.ncbi.nlm.nih.gov/blast/db/FASTA/drosoph.nt.gz"
					}
				}
			}
		},
		"exec": {
			"path": "/opt/soft/blast/bin",
			"method": {
				"blastdbcmd":"/opt/soft/blast/bin/blastdbcmd",
				"samtools": "/opt/soft/blast/bin/samtools"
			}
		},
		"xsl": {
			"blastn": "/opt/apps/seqservice/xsl/blastn.xsl",
			"blastp": "/opt/apps/seqservice/xsl/blastp.xsl"
		},
		"port": "10030",
		"jsonp": false,
		"basepath": "/seqservice"
	}
}

