{
  "name": "seqservice",
  "version": "0.1.1",
  "dependencies": {
	"pouchdb": "6.x",
	"async": "1.x",
	"jquery": "1.12.x",
	"tinysort": "2.x",
	"bootstrap": "3.x",
	"file-saver": "1.x"
  },
	"install" : {
	"path" : {
	  "css": "public/vendor/css",
	  "js": "public/vendor/js"
	},
	"sources" : { 
		"pouchdb": [
			"bower-components/pouchdb/dist/pouchdb.min.js"
		],
		"async": [
			"bower-components/async/dist/async.min.js"
		],
		"jquery": [
			"bower-components/jquery/dist/jquery.min.js"
		],
		"tinysort": [
			"bower-components/tinysort/dist/tinysort.min.js"
		],
		"bootstrap": [
			"bower-components/bootstrap/dist/js/bootstrap.min.js",
			"bower-components/bootstrap/dist/css/bootstrap.min.css",
			"bower-components/bootstrap/dist/css/bootstrap-theme.min.css"
		],
		"file-saver": [
			"bower-components/file-saver/FileSaver.min.js"
		]
		}
